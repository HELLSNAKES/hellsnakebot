{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-redeclare": 0,
        "no-useless-escape": 1,
        "no-empty": 1,
        "no-unused-vars": 1
    }
}
